    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/mapbox-gl": "^2.7.18",
    "@types/react-map-gl": "^6.1.6",
    "axios": "^1.6.1",
    "betterer": "^1.0.2",
