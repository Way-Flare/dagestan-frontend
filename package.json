    "tailwindcss-animate": "^1.0.7",
    "vite-tsconfig-paths": "^4.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/compose-function": "^0.0.32",
